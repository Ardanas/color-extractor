{
  "manifest_version": 3,
  "name": "我的Chrome插件",
  "version": "1.0",
  "description": "这是一个简单的Chrome插件示例",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  }
}